<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.muscu.benjamin.muscu" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.muscu.benjamin.muscu.AcceuilActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.muscu.benjamin.muscu.MesSeancesActivity"
            android:label="@string/title_activity_mes_seances" >
        </activity>
        <activity
            android:name="com.muscu.benjamin.muscu.SeanceActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_seance"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.muscu.benjamin.muscu.LesTypesExercicesActivity"
            android:label="@string/title_activity_exercices" >
        </activity>
        <activity
            android:name="com.muscu.benjamin.muscu.ExerciceActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_exercice"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".TypeExerciceActivity"
            android:label="@string/title_activity_type_exercice" >
        </activity>
    </application>

</manifest>
